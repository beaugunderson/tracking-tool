{
  "name": "tracking-tool",
  "author": "Beau Gunderson <beau@beaugunderson.com>",
  "description": "Social work tracking tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/beaugunderson/tracking-tool.git"
  },
  "version": "7.8.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "build": {
    "appId": "com.beaugunderson.tracking-tool",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.healthcare-fitness",
      "target": "dmg"
    },
    "productName": "Tracking Tool",
    "win": {
      "target": [
        {
          "arch": [
            "ia32",
            "x64"
          ],
          "target": "portable"
        }
      ]
    }
  },
  "scripts": {
    "build": "react-scripts build",
    "dist": "rm -rf dist/ && yarn dist-mac && ./build-windows.sh",
    "dist-mac": "electron-builder --mac",
    "dist-win": "electron-builder --win",
    "eject": "react-scripts eject",
    "eslint": "eslint --config .eslintrc.js --fix --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives src",
    "pack": "electron-builder --dir --mac --win",
    "predist": "yarn build",
    "prepack": "yarn build",
    "postinstall": "electron-builder install-app-deps",
    "start": "concurrently \"cross-env BROWSER=none yarn react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "test": "yarn react-scripts test --env @jest-runner/electron/environment --runner @jest-runner/electron"
  },
  "dependencies": {
    "async": "^3.2.0",
    "camelcase": "^6.0.0",
    "classnames": "^2.2.6",
    "crossfilter2": "^1.5.2",
    "d3": "^5.16.0",
    "dc": "^4.0.5",
    "debug": "^4.1.1",
    "double-metaphone": "^1.0.5",
    "electron-context-menu": "^2.2.0",
    "electron-default-menu": "^1.0.2",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.2",
    "electron-store": "^6.0.0",
    "formik": "^2.1.5",
    "glob": "^7.1.6",
    "merge-img": "^2.1.3",
    "moment": "^2.27.0",
    "moment-range": "^4.0.2",
    "mousetrap": "^1.6.5",
    "nedb": "^1.8.0",
    "node-sass": "^4.14.1",
    "os-name": "^3.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "reductio": "^1.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "slugify": "^1.4.4",
    "typedarray-to-buffer": "^3.1.5",
    "username": "^5.1.0"
  },
  "devDependencies": {
    "@jest-runner/electron": "^3.0.0",
    "@types/async": "^3.2.3",
    "@types/classnames": "^2.2.10",
    "@types/d3": "^5.7.2",
    "@types/dc": "^0.0.29",
    "@types/debug": "^4.1.5",
    "@types/faker": "^4.1.12",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.5",
    "@types/lodash": "^4.14.157",
    "@types/mousetrap": "^1.6.3",
    "@types/nedb": "^1.8.10",
    "@types/node": "^14.0.24",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/string-similarity": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "devtron": "^1.4.0",
    "electron": "^9.1.1",
    "electron-builder": "^22.7.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-react-perf": "^3.2.3",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eslint_d": "^9.1.1",
    "faker": "^4.1.0",
    "prettier": "^2.0.5",
    "release-it": "^13.6.5",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "typescript": "^3.9.7",
    "wait-on": "^5.1.0"
  },
  "browserslist": [
    "Electron 8.0"
  ]
}
