{
  "name": "tracking-tool",
  "author": "Beau Gunderson <beau@beaugunderson.com>",
  "description": "Social work tracking tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/beaugunderson/tracking-tool.git"
  },
  "version": "8.31.0",
  "private": true,
  "main": "dist-electron/main/index.js",
  "build": {
    "appId": "com.beaugunderson.tracking-tool",
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.healthcare-fitness",
      "target": "dmg"
    },
    "productName": "Tracking Tool",
    "win": {
      "target": [
        {
          "arch": [
            "ia32",
            "x64"
          ],
          "target": "portable"
        }
      ],
      "azureSignOptions": {
        "publisherName": "Beau Gunderson",
        "codeSigningAccountName": "personal-signer",
        "certificateProfileName": "personal-trust-profile",
        "endpoint": "https://eus.codesigning.azure.net/"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest --dir src",
    "eslint": "eslint --config .eslintrc.js --fix --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives src",
    "dist": "rm -rf dist/ && ./build-windows.sh",
    "dist-mac": "yarn build && electron-builder --mac",
    "dist-win": "electron-builder --win",
    "postinstall": "electron-builder install-app-deps"
  },
  "resolutions": {
    "fsevents": "^2.3.3",
    "@types/minimatch": "5.1.2"
  },
  "dependencies": {
    "async": "^3.2.2",
    "camelcase": "^6.2.0",
    "classnames": "^2.3.1",
    "crossfilter2": "^1.5.4",
    "d3": "^6.2.0",
    "dc": "^4.2.7",
    "debug": "^4.3.2",
    "double-metaphone": "^2.0.0",
    "electron-context-menu": "^3.1.1",
    "electron-default-menu": "^1.0.2",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.4.1",
    "electron-store": "^8.0.0",
    "formik": "^2.2.9",
    "glob": "^7.1.7",
    "levenshtein-edit-distance": "^3.0.0",
    "merge-img": "^2.1.3",
    "moment": "^2.29.2",
    "moment-range": "^4.0.2",
    "mousetrap": "^1.6.5",
    "nedb": "^1.8.0",
    "parse-full-name": "^1.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reductio": "^1.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3",
    "slugify": "^1.6.0",
    "typedarray-to-buffer": "^4.0.0",
    "xregexp": "^5.1.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.7",
    "@types/classnames": "^2.3.1",
    "@types/d3": "^7.0.0",
    "@types/dc": "^0.0.29",
    "@types/debug": "^4.1.7",
    "@types/faker": "^5.5.8",
    "@types/glob": "^7.1.4",
    "@types/levenshtein-edit-distance": "^3.0.2",
    "@types/lodash": "^4.14.172",
    "@types/mousetrap": "^1.6.8",
    "@types/nedb": "^1.8.12",
    "@types/node": "^22",
    "@types/parse-full-name": "^1.2.2",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/string-similarity": "^4.0.0",
    "@types/xregexp": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "@vitejs/plugin-react": "^4",
    "delectron": "^0.0.1-security",
    "devtron": "^1.4.0",
    "electron": "^33.0.0",
    "electron-builder": "^26.0.17",
    "eslint": "^8.3.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^9",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.31",
    "eslint-plugin-jsx-a11y": "^6.10",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-react": "^7.37",
    "eslint-plugin-react-hooks": "^4.6",
    "eslint-plugin-react-perf": "^3.3",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint_d": "^13",
    "faker": "^5.5.3",
    "jsdom": "^25",
    "prettier": "^3",
    "release-it": "^14.11.6",
    "sass": "^1.43.4",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-no-unsupported-browser-features": "^5.0.1",
    "typescript": "^5",
    "vite": "^6",
    "vite-electron-plugin": "^0.8.3",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^3"
  },
  "browserslist": [
    "Electron 33.0"
  ],
  "packageManager": "yarn@1.22.22+sha256.c17d3797fb9a9115bf375e31bfd30058cac6bc9c3b8807a3d8cb2094794b51ca"
}
