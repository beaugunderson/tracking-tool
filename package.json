{
  "name": "tracking-tool",
  "author": "Beau Gunderson <beau@beaugunderson.com>",
  "description": "Social work tracking tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/beaugunderson/tracking-tool.git"
  },
  "version": "8.33.0",
  "private": true,
  "main": "out/main/index.js",
  "build": {
    "appId": "com.beaugunderson.tracking-tool",
    "files": [
      "out/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.healthcare-fitness",
      "target": "dmg"
    },
    "productName": "Tracking Tool",
    "win": {
      "target": [
        {
          "arch": [
            "ia32",
            "x64"
          ],
          "target": "portable"
        }
      ],
      "azureSignOptions": {
        "publisherName": "Beau Gunderson",
        "codeSigningAccountName": "personal-signer",
        "certificateProfileName": "personal-trust-profile",
        "endpoint": "https://eus.codesigning.azure.net/"
      }
    }
  },
  "scripts": {
    "dev": "electron-vite dev",
    "build": "tsc --noEmit && electron-vite build",
    "test": "vitest",
    "eslint": "eslint --fix --report-unused-disable-directives src/renderer/src",
    "dist-mac": "pnpm build && electron-builder --mac",
    "dist-win": "electron-builder --win",
    "postinstall": "electron-builder install-app-deps"
  },
  "pnpm": {
    "overrides": {
      "fsevents": "^2.3.3",
      "@types/minimatch": "5.1.2",
      "highlight.js@>=9.0.0 <10.4.1": ">=10.4.1",
