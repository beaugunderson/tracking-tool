{
  "name": "tracking-tool",
  "author": "Beau Gunderson <beau@beaugunderson.com>",
  "description": "Social work tracking tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/beaugunderson/tracking-tool.git"
  },
  "version": "8.20.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "build": {
    "appId": "com.beaugunderson.tracking-tool",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.healthcare-fitness",
      "target": "dmg"
    },
    "productName": "Tracking Tool",
    "win": {
      "target": [
        {
          "arch": [
            "ia32",
            "x64"
          ],
          "target": "portable"
        }
      ]
    }
  },
  "scripts": {
    "build": "react-scripts build",
    "dist": "rm -rf dist/ && ./build-windows.sh",
    "dist-mac": "electron-builder --mac",
    "dist-win": "electron-builder --win",
    "eject": "react-scripts eject",
    "eslint": "eslint --config .eslintrc.js --fix --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives src",
    "pack": "electron-builder --dir --mac --win",
    "predist": "yarn build",
    "prepack": "yarn build",
    "postinstall": "electron-builder install-app-deps",
    "start": "concurrently \"cross-env BROWSER=none yarn react-scripts start\" \"wait-on http://127.0.0.1:3000 && electron .\"",
    "test": "yarn react-scripts test --silent --env @jest-runner/electron/environment --runner @jest-runner/electron"
  },
  "dependencies": {
    "async": "^3.2.2",
    "camelcase": "^6.2.0",
    "classnames": "^2.3.1",
    "crossfilter2": "^1.5.4",
    "d3": "^6.2.0",
    "dc": "^4.2.7",
    "debug": "^4.3.2",
    "double-metaphone": "^2.0.0",
    "electron-context-menu": "^3.1.1",
    "electron-default-menu": "^1.0.2",
    "electron-devtools-installer": "^3.2.0",
    "electron-find": "^1.0.7",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.1",
    "electron-store": "^8.0.0",
    "formik": "^2.2.9",
    "glob": "^7.1.7",
    "levenshtein-edit-distance": "^3.0.0",
    "merge-img": "^2.1.3",
    "moment": "^2.29.2",
    "moment-range": "^4.0.2",
    "mousetrap": "^1.6.5",
    "nedb": "^1.8.0",
    "os-name": "^4.0.0",
    "parse-full-name": "^1.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "reductio": "^1.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3",
    "slugify": "^1.6.0",
    "typedarray-to-buffer": "^4.0.0",
    "username": "^5.1.0",
    "xregexp": "^5.1.0"
  },
  "devDependencies": {
    "@jest-runner/electron": "^3.0.1",
    "@types/async": "^3.2.7",
    "@types/classnames": "^2.3.1",
    "@types/d3": "^7.0.0",
    "@types/dc": "^0.0.29",
    "@types/debug": "^4.1.7",
    "@types/faker": "^5.5.8",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.1",
    "@types/levenshtein-edit-distance": "^3.0.2",
    "@types/lodash": "^4.14.172",
    "@types/mousetrap": "^1.6.8",
    "@types/nedb": "^1.8.12",
    "@types/node": "^16.7.10",
    "@types/parse-full-name": "^1.2.2",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/string-similarity": "^4.0.0",
    "@types/xregexp": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-eslint": "^10.1.0",
    "caniuse-lite": "^1.0.30001376",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "delectron": "^0.0.1-security",
    "devtron": "^1.4.0",
    "electron": "^13.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-perf": "^3.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint_d": "^11.0.0",
    "faker": "^5.5.3",
    "prettier": "^2.3.2",
    "release-it": "^14.11.6",
    "sass": "^1.43.4",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-no-unsupported-browser-features": "^5.0.1",
    "typescript": "^4.4.2",
    "wait-on": "^6.0.0"
  },
  "browserslist": [
    "Electron 8.0"
  ],
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
