{
  "name": "tracking-tool",
  "author": "Beau Gunderson <beau@beaugunderson.com>",
  "description": "Social work tracking tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/beaugunderson/tracking-tool.git"
  },
  "version": "3.9.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "build": {
    "appId": "com.beaugunderson.tracking-tool",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.healthcare-fitness",
      "target": "zip"
    },
    "productName": "Tracking Tool",
    "win": {
      "target": [
        {
          "arch": [
            "ia32",
            "x64"
          ],
          "target": "portable"
        }
      ]
    }
  },
  "scripts": {
    "build": "react-scripts build",
    "dist": "rm -rf dist/ && electron-builder --mac --win",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir --mac --win",
    "predist": "yarn build",
    "prepack": "yarn build",
    "postinstall": "electron-builder install-app-deps",
    "start": "concurrently \"cross-env BROWSER=none yarn react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "camelcase": "^5.0.0",
    "crossfilter2": "https://github.com/crossfilter/crossfilter",
    "d3": "^5.7.0",
    "dc": "^3.0.9",
    "debug": "^4.1.1",
    "electron-context-menu": "^0.10.1",
    "electron-default-menu": "^1.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^1.0.1",
    "electron-store": "^2.0.0",
    "formik": "^1.4.2",
    "glob": "^7.1.3",
    "moment": "^2.23.0",
    "mousetrap": "^1.6.2",
    "nedb": "^1.8.0",
    "os-name": "^3.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "^2.1.3",
    "reductio": "^0.6.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.84.0",
    "slugify": "^1.3.4",
    "username": "^4.0.0"
  },
  "devDependencies": {
    "@types/camelcase": "^4.1.0",
    "@types/d3": "^5.5.0",
    "@types/dc": "^0.0.28",
    "@types/debug": "^0.0.31",
    "@types/faker": "^4.1.4",
    "@types/glob": "^7.1.1",
    "@types/jest": "^23.3.12",
    "@types/lodash": "^4.14.119",
    "@types/mousetrap": "^1.6.0",
    "@types/nedb": "^1.8.7",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "babel-eslint": "^9.0.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "devtron": "^1.4.0",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.4",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-react-perf": "^3.0.1",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint_d": "^7.2.0",
    "faker": "^4.1.0",
    "prettier": "^1.15.3",
    "release-it": "^9.8.0",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^21.0.2",
    "wait-on": "^3.2.0"
  },
  "browserslist": [
    "Electron 4.0"
  ]
}
