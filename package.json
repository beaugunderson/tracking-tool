      "qs@<6.14.1": ">=6.14.1"
    }
  },
  "dependencies": {
    "@seald-io/nedb": "^4.1.2",
    "async": "^3.2.2",
    "camelcase": "^9.0.0",
    "classnames": "^2.3.1",
