{
  "name": "tracking-tool",
  "author": "Beau Gunderson <beau@beaugunderson.com>",
  "description": "Social work tracking tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/beaugunderson/tracking-tool.git"
  },
  "version": "2.0.2",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "build": {
    "appId": "com.beaugunderson.tracking-tool",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.healthcare-fitness",
      "target": "zip"
    },
    "productName": "Tracking Tool",
    "win": {
      "target": "portable"
    }
  },
  "scripts": {
    "build": "react-scripts build",
    "dist": "rm -rf dist/ && electron-builder --mac --win",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir --mac --win",
    "predist": "yarn build",
    "prepack": "yarn build",
    "postinstall": "electron-builder install-app-deps",
    "start": "concurrently \"cross-env BROWSER=none yarn react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "camelcase": "^5.0.0",
    "debug": "^4.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^1.0.1",
    "electron-store": "^2.0.0",
    "formik": "^1.3.2",
    "moment": "^2.22.2",
    "nedb": "^1.8.0",
    "os-name": "^3.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.83.0",
    "slugify": "^1.3.3",
    "username": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "devtron": "^1.4.0",
    "electron": "^3.0.10",
    "electron-builder": "^20.36.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-perf": "^2.0.9",
    "eslint_d": "^7.1.0",
    "flow-bin": "^0.81.0",
    "prettier": "^1.14.2",
    "release-it": "^8.2.0",
    "stylelint": "^9.8.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "wait-on": "^3.2.0"
  },
  "browserslist": [
    "Electron 3.0"
  ]
}
